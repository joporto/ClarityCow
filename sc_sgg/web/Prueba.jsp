            $(document).ready(function (e) {

                // Fecha Inicio y Final
                $('#finicio, #ffinal').datepicker({language: 'es'});

                // Actualiza de forma automatica
                Actualizar(false);

                // Actualiza Centros de Costo
                actCentros();

                // Actualiza Centro de Costo
                function actCentros() {

                    // Recupera Planta
                    var idpla = $('#pla_id').val();

                    // Inicializa
                    $('#cen_id').empty();
                    $('#cen_id').html('<option value="">Todos</option>');

                    // Valida
                    if (idpla == '')
                        idpla = null;

                    // LLama Servicio
                    $.post("http://demo.claritymachine.cl/" + "centro/CentrosxPlantaJSON", {pla_id: idpla}, function (res) {

                        // Parsea
                        var json = $.parseJSON(res);

                        // Muestra los resultados
                        var datos = json;
                        var lp = datos.length;
                        for (var i = 0; i < lp; i++) {
                            var opt = '<option value=' + datos[i].cen_id + '>' + datos[i].cen_descripcion + '</option>';
                            $('#cen_id').append(opt);
                        }
                    });
                }


                // Actualiza el Boton
                function Actualizar(mensaje) {

                    // Recupera Informacion
                    var idpla = $('#pla_id').val();
                    var idcen = $('#cen_id').val();
                    var idcenmaq = $('#maq_id').val();
                    var finicio = $('#finicio').val();
                    var ffinal = $('#ffinal').val();

                    $.post("http://demo.claritymachine.cl/" + "otrabajo/ResumenOTJSON", {pla_id: idpla, cen_id: idcen, maq_id: idcenmaq, fechaini: finicio, fechafin: ffinal}, function (res) {

                        // Parsea
                        var json = $.parseJSON(res);


                        // Verifica si trae Resultados
                        if (json.ok == 'ok') {

                            // Paradas
                            var lparadas = json.paradas;
                            var lp = lparadas.length;
                            var newtr = '';

                            // Limpia
                            $('#ActParada').html('');

                            var total = 0;

                            for (var i = 0; i < lp; i++) {
                                total = total + (parseInt(lparadas[i].contpara));
                            }


                            // Recorre
                            for (var i = 0; i < lp; i++) {
                                /* 
                                 // Crea Registros
                                 newtr = '<tr class="item">';
                                 newtr = newtr + '<td>' + lparadas[i].codigo + ' | ' + lparadas[i].maq_nombre + '</td>';
                                 newtr = newtr + '<td>' + lparadas[i].contpara + '</td>';
                                 newtr = newtr + '</tr>';*/

                                var div = $('<div class="progress-stat">');
                                var label = $('<div class="progress-stat-label">');
                                $(label).html(lparadas[i].maq_nombre + ' | ' + lparadas[i].codigo);
                                $(div).append(label);

                                var per = Math.floor((parseInt(lparadas[i].contpara) / total) * 100);


                                var pst = $('<div class="progress-stat-value">');
                                $(pst).html(lparadas[i].contpara);
                                $(div).append(pst);
                                var pro = $('<div class="progress">');
                                if (per >= 50) {
                                    var prog = $('<div class="progress-bar progress-bar-danger animated" role="progressbar" style="width:' + per + '%"></div>');
                                } else {
                                    var prog = $('<div class="progress-bar progress-bar-success  animated" role="progressbar" style="width:' + per + '%"></div>');
                                }

                                $(pro).append(prog);
                                $(div).append(pro);

                                // Agrega  
                                $('#ActParada').append(div);
                            }

                            // Horas Hombre
                            var lhh = json.hh;
                            var lp = lhh.length;
                            var newtr = '';

                            // Limpia
                            $('#Acthh').html('');


                            var totalhh = 0;

                            for (var i = 0; i < lp; i++) {

                                totalhh = totalhh + (parseInt(lhh[i].horash));

                            }

                            // Recorre
                            for (var i = 0; i < lp; i++) {

                                /*// Crea Registros
                                 newtr = '<tr class="item">';
                                 newtr = newtr + '<td>' + lhh[i].codigo + ' | ' + lhh[i].maq_nombre + '</td>';
                                 newtr = newtr + '<td>' + lhh[i].horash + '</td>';
                                 newtr = newtr + '</tr>';*/

                                var div = $('<div class="progress-stat">');
                                var label = $('<div class="progress-stat-label">');
                                $(label).html(lhh[i].maq_nombre + ' | ' + lhh[i].codigo);
                                $(div).append(label);

                                var per = Math.floor((parseInt(lhh[i].horash) / totalhh) * 100);

                                var pst = $('<div class="progress-stat-value">');
                                $(pst).html(lhh[i].horash);
                                $(div).append(pst);
                                var pro = $('<div class="progress">');
                                if (per >= 50) {
                                    var prog = $('<div class="progress-bar progress-bar-danger animated" role="progressbar" style="width:' + per + '%"></div>');
                                } else {
                                    var prog = $('<div class="progress-bar progress-bar-success animated" role="progressbar" style="width:' + per + '%"></div>');
                                }
                                $(pro).append(prog);
                                $(div).append(pro);

                                // Agrega  
                                $('#Acthh').append(div);
                            }

                            // Repuestos
                            var lRepuestos = json.repuestos;
                            var lp = lRepuestos.length;
                            var newtr = '';
                            var totalrep = 0;

                            // Limpia
                            $('#ActRepuesto').html('');

                            for (var i = 0; i < lp; i++) {

                                totalrep = totalrep + (parseInt(lRepuestos[i].cantid));

                            }

                            // Recorre
                            for (var i = 0; i < lp; i++) {

                                var div = $('<div class="progress-stat">');
                                var label = $('<div class="progress-stat-label">');
                                $(label).html(lRepuestos[i].rep_nombre + ' | ' + lRepuestos[i].rep_modelo);
                                $(div).append(label);

                                var per = Math.floor((parseInt(lRepuestos[i].cantid) / totalrep) * 100);

                                var pst = $('<div class="progress-stat-value">');
                                $(pst).html(lRepuestos[i].cantid);
                                $(div).append(pst);
                                var pro = $('<div class="progress">');
                                if (per >= 50) {
                                    var prog = $('<div class="progress-bar progress-bar-danger animated" role="progressbar" style="width:' + per + '%"></div>');
                                } else {
                                    var prog = $('<div class="progress-bar progress-bar-success animated" role="progressbar" style="width:' + per + '%"></div>');
                                }
                                $(pro).append(prog);
                                $(div).append(pro);


                                // Agrega  
                                $('#ActRepuesto').append(div);
                            }


                            // Horas de Parada
                            var lhorasp = json.horasp;
                            var lp = lhorasp.length;
                            var newtr = '';
                            var totalhoras = 0;

                            // Limpia
                            $('#ActHorasP').html('');

                            for (var i = 0; i < lp; i++) {

                                totalhoras = totalhoras + (parseInt(lhorasp[i].horas));

                            }

                            // Recorre
                            for (var i = 0; i < lp; i++) {

                                var div = $('<div class="progress-stat">');
                                var label = $('<div class="progress-stat-label">');
                                $(label).html(lhorasp[i].codigo + ' | ' + lhh[i].maq_nombre);
                                $(div).append(label);

                                var per = Math.floor((parseInt(lhorasp[i].horas) / totalhoras) * 100);

                                var pst = $('<div class="progress-stat-value">');
                                $(pst).html(lhorasp[i].horas);
                                $(div).append(pst);
                                var pro = $('<div class="progress">');
                                if (per >= 50) {
                                    var prog = $('<div class="progress-bar progress-bar-danger animated" role="progressbar" style="width:' + per + '%"></div>');
                                } else {
                                    var prog = $('<div class="progress-bar progress-bar-success animated" role="progressbar" style="width:' + per + '%"></div>');
                                }
                                $(pro).append(prog);
                                $(div).append(pro);


                                // Agrega  
                                $('#ActHorasP').append(div);
                            }


                            // Tecnico
                            var lTecnicos = json.tecnicos;
                            var lp = lTecnicos.length;
                            var newtr = '';
                            var horasTenico = 0;

                            // Limpia
                            $('#ActTecnico').html('');

                            for (var i = 0; i < lp; i++) {

                                horasTenico = horasTenico + (parseInt(lTecnicos[i].horash));

                            }

                            // Recorre
                            for (var i = 0; i < lp; i++) {

                                /*// Crea Registros
                                 newtr = '<tr class="item">';
                                 newtr = newtr + '<td>' + lTecnicos[i].usu_nombre + '</td>';
                                 newtr = newtr + '<td>' + lTecnicos[i].horash + '</td>';
                                 newtr = newtr + '</tr>';*/

                                var div = $('<div class="progress-stat">');
                                var label = $('<div class="progress-stat-label">');
                                $(label).html(lTecnicos[i].usu_nombre);
                                $(div).append(label);

                                var per = Math.floor((parseInt(lTecnicos[i].horash) / horasTenico) * 100);

                                var pst = $('<div class="progress-stat-value">');
                                $(pst).html(lTecnicos[i].horash);
                                $(div).append(pst);
                                var pro = $('<div class="progress">');
                                if (per >= 50) {
                                    var prog = $('<div class="progress-bar progress-bar-danger animated" role="progressbar" style="width:' + per + '%"></div>');
                                } else {
                                    var prog = $('<div class="progress-bar progress-bar-success animated" role="progressbar" style="width:' + per + '%"></div>');
                                }
                                $(pro).append(prog);
                                $(div).append(pro);

                                // Agrega  
                                $('#ActTecnico').append(div);
                            }
                        }
                        else {

                            // Se imprime el mensaje
                            if (mensaje) {
                                // No hay datos
                                toastr.error("No hay datos con los parámetros establecidos ...");

                            }

                            // Limpia
                            $('#ActParada').html('');
                            $('#Acthh').html('');
                            $('#ActHorasP').html('');
                            $('#ActTecnico').html('');
                            $('#ActRepuesto').html('');
                        }
                    });
                }


                // Valida algun cambio en Planta
                $('#pla_id').change(function (e) {

                    // Actualiza Centros de Costo
                    actCentros();
                });


                // Valida algun cambio en Centro de Costo
                $('#cen_id').change(function (e) {

                    // Recupera Centro			
                    var idcen = $(this).val();

                    // Inicializa
                    $('#maq_id').empty();
                    $('#maq_id').html('<option value="">Todas</option>');

                    // Esta Vacio
                    if (idcen == '')
                        idcen = null;

                    // Servicio
                    $.post("http://demo.claritymachine.cl/" + "maquina/MaquinasxCentroJSON", {cen_id: idcen}, function (res) {

                        // Parsea
                        var json = $.parseJSON(res);

                        // Muestra los resultados
                        var datos = json;
                        var lp = datos.length;
                        for (var i = 0; i < lp; i++) {
                            var opt = "<option value='" + datos[i].id_centro_maquina + "'>" + datos[i].codigo + " | " + datos[i].maq_nombre + "</option>";
                            $('#maq_id').append(opt);
                        }
                    });
                });


                // Actualiza el Boton
                $('#reload').click(function (e) {

                    // Actualizar al hacer click
                    Actualizar(true);
                });

            });

        </script>


